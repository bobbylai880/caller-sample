  "dependencies": {
    "@asteasolutions/zod-to-openapi": "^6.3.0",
    "@prisma/client": "^5.14.0",
    "bullmq": "^4.14.0",
    "google-auth-library": "^9.8.0",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "http-status-codes": "^2.3.0",
    "ioredis": "^5.4.1",
    "morgan": "^1.10.0",
    "openapi3-ts": "^4.3.1",
    "swagger-ui-express": "^5.0.0",
    "twilio": "^4.22.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/morgan": "^1.9.7",
    "@types/node": "^20.12.7",
    "@typescript-eslint/eslint-plugin": "^7.7.0",
    "@typescript-eslint/parser": "^7.7.0",
    "eslint": "^8.57.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "openapi-typescript": "^6.6.0",
    "prisma": "^5.14.0",
    "tsx": "^4.7.1",
    "typescript": "^5.4.5",
    "vitest": "^1.5.0",
    "supertest": "^6.3.4",
